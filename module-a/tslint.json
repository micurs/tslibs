{
  "extends": "tslint:recommended",
  "rules": {
    "indent": [true, "spaces"],
    "interface-name": [false],
    "semicolon": [true, "always", "ignore-bound-class-methods"],
    "quotemark": [true, "single", "jsx-double"],
    "ordered-imports": [false],
    "whitespace": [ true, "check-operator", "check-typecast" ]
  }
}
